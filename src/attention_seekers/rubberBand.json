        {
            "type": "AnimateItem",
            "duration": "${duration * 0.1}",
            "value": [
                {
                    "property": "transform",
                    "from": [
                        {
                            "scaleX": 0.95,
                            "scaleY": 1.05
                        }
                    ],
                    "to": [
                        {
                            "scaleX": 1.05,
                            "scaleY": 0.95
                        }
                    ]
                }
            ]
        },
        {
            "type": "AnimateItem",
            "duration": "${duration * 0.25}",
            "value": [
                {
                    "property": "transform",
                    "from": [
                        {
                            "scaleX": 1.05,
