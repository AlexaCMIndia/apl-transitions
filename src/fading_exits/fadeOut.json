{
    "parameters": [
        "duration",
        "delay"
    ],
    "commands": [
        {
            "type": "AnimateItem",
            "duration": "${duration}",
            "delay": "${delay || 0}",
            "value": [
                {
                    "property": "opacity",
                    "from": 1,
                    "to": 0
                }
            ]
        }
    ]
}